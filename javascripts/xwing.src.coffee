    if (data.name == 'Y-Wing' and faction == 'Scum and Villainy')
        return false
    else if (data.name == 'TIE Fighter' and faction == 'Rebel Alliance')
        return false
    for source in data.sources
        return true if source in exportObj.secondEditionExpansions
    false

